/* discovery.wxss */

.item-title {
  text-align: left;
  padding: 10px;
  border-bottom: 1px dotted #e7e7e7;
  margin-bottom: 10px;
}

.item-cell {
  flex: 1;
  display: block;
  padding: 10px;
  margin-left: 10px;
  margin-top: 10px;
  text-align: center;
}

.item-border {
  border: 1px solid #ccc;
  border-radius: 5px;
}

.item-last {
  margin-right: 10px;
}
